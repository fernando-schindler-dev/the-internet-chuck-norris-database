{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        // "plugin:react/recommended",
        "airbnb-base",
        "prettier"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "no-unused-vars": "off",
        "no-await-in-loop":"off",
        "no-unsafe-finally": "off",
        "no-unused-expressions": "off"
    }
}
